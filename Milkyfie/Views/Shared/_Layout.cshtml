                </ol>

                <ul class="app-actions">
                    <li>
                        <a href="#" id="reportrange">
                            <span class="date-time"></span>
                           
                        </a>
                    </li>
                    <li>
                        <a href="~/Account/Logout">
                            <i class="icon-export"></i>
                        </a>
                    </li>
                </ul>
            </div>
            <!-- Page header end -->
            <!-- Main container start -->
            @RenderBody()
            <!-- Main container end -->

        </div>
