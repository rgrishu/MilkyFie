@{
    ViewData["Title"] = "Users";
    Layout = "_LayOut";
}
<div class="main-container">
    <!-- Row start -->
    <div class="row gutters">
        <div id="frmcat" class="w-100">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="card">
                    <div class="card-header bg-primary text-white h6">
                        Users
                        <div class="float-right">  <button id="btnNew" class="btnaddUser btn btn-info">New</button></div>
                    </div>
                    <div class="card-body">
                        <div class="row gutters">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>
                                            #
                                        </th>

                                        <th>
                                            User Name
                                        </th>
                                          <th>
                                            EmailID
                                        </th>
                                        <th>
                                            Mobile
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts{
<script>
    $(document).ready(function(){
     getUsers();
    })
   
     $('.btnaddUser').click(function(){
           Q.preloader.load();
              $.ajax({
                type: 'post',
                url: '/Master/UserForm',
                success: function (data) {
                    console.log(data)
                Q.alert({
                    title: "Add User",
                    body:data,
                    width : '900px',
                    });
                      Q.preloader.remove();
                },
                error: function (data) {
                    console.log('An error occurred.');
                    console.log(data);
                        Q.notify(-1, 'An error occurred.');
                         Q.preloader.remove();
                },
            });
    });
    function getUsers(){
         Q.preloader.load();
         $.ajax({
               type: 'post',
                url: 'Users',
                success: function (data) {
                      $('tbody').html(data);
                },
                error: function (data) {
                    console.log('An error occurred.');
                    console.log(data);
                       Q.notify(-1, 'An error occurred.');
                },
          });
          Q.preloader.remove();
    }


</script>
}